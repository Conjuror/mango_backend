<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <version>1</version>
  <threadsafe>true</threadsafe>
  <runtime>java8</runtime>
  <instance-class>@GAE_INSTANCE_CLASS@</instance-class>
  <automatic-scaling>
    <min-instances>@MIN_INSTANCES@</min-instances>
    <max-instances>@MAX_INSTANCES@</max-instances>
    <target-throughput-utilization>0.9</target-throughput-utilization>
    <max-concurrent-requests>80</max-concurrent-requests>
    <min-pending-latency>6s</min-pending-latency>
    <max-pending-latency>9s</max-pending-latency>
  </automatic-scaling>
  <system-properties>
    @SPRING_PROFILES_ACTIVE@
  </system-properties>
</appengine-web-app>